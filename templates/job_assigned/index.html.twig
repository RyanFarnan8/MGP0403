{% extends 'base.html.twig' %}

{% block title %}JobAssigned index{% endblock %}

{% block body %}
    <h1>JobAssigned index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Contact</th>
                <th>Description</th>
                <th>Location</th>
                <th>Price</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for job_assigned in job_assigneds %}
            <tr>
                <td>{{ job_assigned.id }}</td>
                <td>{{ job_assigned.contact }}</td>
                <td>{{ job_assigned.description }}</td>
                <td>{{ job_assigned.location }}</td>
                <td>{{ job_assigned.price }}</td>
                <td>
                    <a href="{{ path('job_assigned_show', {'id': job_assigned.id}) }}">show</a>
                    <a href="{{ path('job_assigned_edit', {'id': job_assigned.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('job_assigned_new') }}">Create new</a>
{% endblock %}
