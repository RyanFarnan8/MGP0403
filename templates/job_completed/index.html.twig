{% extends 'base.html.twig' %}

{% block title %}JobCompleted index{% endblock %}

{% block body %}
    <h1>JobCompleted index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for job_completed in job_completeds %}
            <tr>
                <td>{{ job_completed.id }}</td>
                <td>{{ job_completed.description }}</td>
                <td>
                    <a href="{{ path('job_completed_show', {'id': job_completed.id}) }}">show</a>
                    <a href="{{ path('job_completed_edit', {'id': job_completed.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('job_completed_new') }}">Create new</a>
{% endblock %}
